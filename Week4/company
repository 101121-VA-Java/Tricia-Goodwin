drop table if exists Customers;
create table if not exists Customers(

	c_id serial primary key,
	c_fname varchar(20),
	c_lname varchar(20),
	c_username varchar(20) unique,
	c_role varchar(15),
	c_password varchar(20),
	c_books int
);


