

drop table if exists books;
create table if not exists books(
	b_title varchar(75),
	b_author varchar(25),
	b_isbn integer primary key,
	b_released boolean,
	b_stock integer,
	b_genre varchar(50),
	b_price decimal(4,2),
	b_hardcover boolean,
	b_nytb boolean
);
insert into books ()
drop table if exists tasks;
create table if not exists tasks(
	/*
	 * id
	 * name
	 * description
	 * completion_status
	 * due_date
	 * employee
	 */
	id serial primary key,
	t_name varchar(20),
	t_description text,
	t_completion_status varchar(10),
	t_assign_date date default current_date,
	t_due_date date,
	t_assigned_emp integer references employees(e_id)
);

