drop table if exists Booksellers;
create table if not exists Booksellers(

	bs_emp_id serial primary key,
	bs_fname varchar(20),
	bs_lname varchar(20),
	bs_username varchar(20) unique,
	bs_role varchar(15),
	bs_password varchar(20),
	bs_hrly decimal(4,2)
);


